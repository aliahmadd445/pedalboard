{
    "name": "pedalboard",
    "version": "1.0.0",
    "description": "A collection of packages to help you express you software better",
    "main": "index.js",
    "author": "Matti Bar-Zeev",
    "license": "MIT",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "test": "yarn workspaces foreach -p run test",
        "coverage": "yarn test --coverage",
        "lint": "yarn workspaces foreach -p run lint",
        "build": "yarn workspaces foreach -ptv run build",
        "publish:lerna": "lerna publish --yes --no-verify-access",
        "coverage:combined": "pedalboard-scripts aggregatePackagesCoverage && nyc report --reporter lcov"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.17.12",
        "@pedalboard/scripts": "^0.1.1",
        "lerna": "^5.0.0",
        "nyc": "^15.1.0"
    },
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "packageManager": "yarn@3.2.0"
}
