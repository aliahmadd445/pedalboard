{
    "name": "pedalboard",
    "version": "1.0.0",
    "description": "A collection of packages to help you express you software better",
    "main": "index.js",
    "author": "Matti Bar-Zeev",
    "license": "MIT",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "test": "lerna run test --",
        "coverage": "yarn test --coverage",
        "lint": "lerna run lint",
        "publish:lerna": "lerna publish --yes --no-verify-access",
        "coverage:combined": "node ./scripts/aggregate-packages-coverage.js && nyc report --reporter lcov"
    },
    "devDependencies": {
        "lerna": "^4.0.0",
        "nyc": "^15.1.0"
    },
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
