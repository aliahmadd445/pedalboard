{
    "name": "pedalboard",
    "version": "1.0.0",
    "description": "A collection of packages to help you express you software better",
    "main": "index.js",
    "author": "Matti Bar-Zeev",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/mbarzeev/pedalboard.git"
    },
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "test": "yarn workspaces foreach --since -pR run test",
        "lint": "yarn workspaces foreach --since -pR run lint",
        "build": "yarn workspaces foreach --since -ptvR run build",
        "publish:lerna": "lerna publish --yes --no-verify-access",
        "publish:lerna:skip-git": "lerna publish --yes --no-verify-access --no-git-tag-version --no-push",
        "coverage": "yarn test --coverage",
        "coverage:combined": "pedalboard-scripts aggregatePackagesCoverage && nyc report --reporter lcov"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.17.12",
        "@pedalboard/scripts": "^0.1.1",
        "lerna": "^5.0.0",
        "nyc": "^15.1.0"
    },
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "packageManager": "yarn@3.2.0"
}
